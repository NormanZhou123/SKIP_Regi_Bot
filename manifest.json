{
  "manifest_version": 2,

  "name": "SKIP Regi Bot",
  "description": "This extension will help you get the $7 off coupon and register a new account for you",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://www.skipthedishes.com/"],
      "js": ["content.js"]
    }
  ],

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}
